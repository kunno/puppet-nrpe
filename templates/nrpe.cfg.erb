# Managed by Puppet <%= @module_name %>.
<%-
@settings.keys.sort.each do |k|
    case @settings[k].class.to_s
    when 'Array' then v = @settings[k].join(',')
    when 'Fixnum' then v = @settings[k]
    when 'TrueClass','FalseClass' then v = @settings[k] ? '1' : '0'
    else v = scope.function_shellquote([@settings[k]])
    end
-%>
<%= k %>=<%= v %>
<%- end -%>
